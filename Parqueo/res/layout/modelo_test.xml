<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:padding="10dp">

    <EditText
        android:id="@+id/etHoraEntradaTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:text="08:30:00">

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/etFechaEntradaTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10" 
        android:text="2015-05-04"/>
    
    <EditText
        android:id="@+id/etHoraSalidaTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:text="19:30:00">

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/etFechaSalidaTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10" 
        android:text="2015-05-04"/>

    <TextView
        android:id="@+id/tvMensajeTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aca va el mensaje" />

    <Button
        android:id="@+id/btnProbarTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Probar" />

</LinearLayout>
